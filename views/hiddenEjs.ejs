<% for (let topping in result) { %>
  <tr class="pizza">
    <td>
      <div class="item-container">
        <p>
          <%=result[topping].name %>*
        </p>
        <img src="https://freepngimg.com/thumb/pizza/35510-7-pepperoni-pizza.png">
      </div>
    </td>
    <% if (result[topping].topping_category_id=1){ %>
      <td>
        <div class="check-group">
          <input class="check" type="checkbox">
          <!-- <% if (<cookietoppingarray>>.includes() ){ checked }%> -->
          <label>
            <%=result[topping].name %>
          </label>
        </div>
      </td>
      <%}%>
        <td>
          <div class="check-group">
            <input class="check" type="checkbox">
            <label>Meats-shoutout the zucck</label>
          </div>
        </td>
        <td>
          <div class="check-group">
            <input class="check" type="checkbox">
            <label>veggies</label>
          </div>

        </td>
        <td>
          <div>
            <form>
              <select name="size" id="size">
                <option value="byte">byte</option>
                <option value="megabyte">megabyte</option>
                <option value="gigabyte">gigabyte</option>
              </select>
            </form>
          </div>
        </td>
        <div>
          <td class="last">1 BTC</td>
        </div>
  </tr>
  <% } %>
